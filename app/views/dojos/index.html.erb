<h1>List 3 Dojos</h1>



<table id="customers">
  <thead>
    <tr>
      <td>Coding Dojo Branch</td>
      <td>Street</td>
      <td>City</td>
      <td>State</td>
      <td>Action</td>
    </tr>
  </thead>
  <tbody>
  <% @dojos.each do |u| %>
    <tr>
      <td><%= u.branch %></td>
      <td><%= u.street %></td>
      <td><%= u.city %></td>
      <td><%= u.state %></td>
      <td><a href="dojos/<%=u.id %> ">Show</a> ||<a href="/dojos/<%= u.id %>/edit">Edit</a>||<a href="/dojos/<%= u.id %>/delete "data-method="delete">delete</a> </td>
    </tr>
  <% end %>  
  </tbody>
</table>
<a href="dojos/new">New Dojo</a>